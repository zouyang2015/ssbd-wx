/* pages/detail/detail.wxss */
page {
  width: 100vw;
  height: 100vh;
  background: #efeff4;
}
.detail-navi {
  display: flex;
  justify-content: center;
  height: 88rpx;
  box-sizing: border-box;
  overflow: hidden;
  border-top: 1px solid #e6e6e6;
  border-bottom: 1px solid #e6e6e6;
  padding: 20rpx 30rpx;
  background: #ffffff;
}
.page-list {
  flex: 1;
  text-align: center;
  font-size: 28rpx;
  border-left: 1px solid #e6e6e6;
}
.page-list:first-child {
  border-left: none;
}
.profit-text {
  padding: 0 0 0 30rpx;
  background:url('data:image/jpeg;base64,') no-repeat left;
  background-size: 22rpx 26rpx;
}
.clause-text {
  padding: 0 0 0 30rpx;
  background:url('data:image/jpeg;base64,') no-repeat left;
  background-size: 22rpx 26rpx;
}
.rule-text {
  padding: 0 0 0 30rpx;
  background:url('data:image/jpeg;base64,') no-repeat left;
  background-size: 22rpx 26rpx;
}